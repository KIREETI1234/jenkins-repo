pipeline {
    agent any
    
    stages {
        stage('Clone Repository') {
            steps {
                sh 'git clone -b main https://github.com/KIREETI1234/jenkins-repo.git'  
            }
        }
        stage('Build') {
            steps {
                sh 'echo build successful'
        }
      }
    }
}

// Save this script as a Jenkinsfile and use it in a Jenkins pipeline job.
